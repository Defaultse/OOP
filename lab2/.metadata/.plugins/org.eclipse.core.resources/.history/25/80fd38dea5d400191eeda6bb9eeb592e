package probem5;

public class Iphone {
	
static String model;
static int color;

public enum Color{
	1("Space Grey"),
	2("Matte Black"),
	3("Rose Gold"),
	4("White")
}

Iphone(){
model = "not chosen";
color = 0;
}

Iphone(String model, int color){
	this.model = model;
	this.color = color;
}

static void info() {
	System.out.println(model+" "+Color.color);
}
