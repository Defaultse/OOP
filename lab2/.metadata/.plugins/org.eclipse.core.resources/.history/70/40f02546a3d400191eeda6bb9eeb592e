package probem5;

public class Iphone {
	
String model;
int color;

public enum Color{
	SPACEGREY(1),
	JETBLACK(2),
	MATTEBLACK(3),
	ROSEGOLD(4)
}

Iphone(){
model = "not chosen";
color = 0;
}

Iphone(String model, int color){
	this.model = model;
	this.color = color;
}

static void info() {
	System.out.println(model+" "+color);
}
