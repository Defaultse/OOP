package problem2;

public class StarTriangle {
public int width;
StarTriangle(int numberOfStar){
	width = numberOfStar;
}

public String toString(){  
		for (int i=1; i <= width; i++)
		{
		    for (int j=1; j<=width; j++)
		    {
		    	 if(i==j || i>=j) return "[*]";   
		    	 else return " ";
		    }		
		}
		return " ";
	
	
}
}
