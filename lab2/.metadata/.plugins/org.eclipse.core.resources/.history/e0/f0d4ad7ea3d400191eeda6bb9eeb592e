package probem5;

public class Iphone {
	
static String model;
static String color;

public enum Color{
	SPACEGREY("серый"),
	JETBLACK("блестящий черный"),
	MATTEBLACK("матовый черный"),
	ROSEGOLD("розовый")
}

Iphone(){
model = "not chosen";
color = "not chosen";
}

Iphone(String model, int color){
	this.model = model;
	this.color = color;
}

static void info() {
	System.out.println(model+" "+color);
}
