package problem3;

public class Data {
private double maximum;
private double average;
private int number;

Data(int num) {	
	this.number = num;   
}

public void addValue(int num){
	
}

public double findAverage(){
this.average=(average+=number)/2;
}

public double findMax() {
	this.maximum = (Math.max(maximum,number));
}

}
