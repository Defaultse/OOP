package problem2;

public class StarTriangle {
	
public int width;
StarTriangle(int numberOfStar){
	width = numberOfStar;
}

public void toString(){  
	for(int i = 1; i <= width; i++)
    {
            for(int j = 1; j <= i; j++)
            {
              System.out.print("[*]");  
            }
        System.out.println();
    }
}

}
