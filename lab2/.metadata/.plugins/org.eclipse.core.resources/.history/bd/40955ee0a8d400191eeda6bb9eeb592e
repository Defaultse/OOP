package probem5;

public class Iphone {
	
static String model;
static String color;

public enum Color{
	first("Space Grey"),
	second("Matte Black"),
	third("Rose Gold"),
	fourth("White"),
	no
}

Iphone(){
model = "not chosen";
color = "no";
}

Iphone(String model, String color){
	this.model = model;
	this.color = color;
}
static String getModel() {
	return model;
}

static String getColor() {
	return color;
}